{
  "data_reference": {
    "snippet": "<comp.DataReference>\n[[rows]]</comp.DataReference>",
    "inline": false,
    "preview": {
      "text": "Data Reference Table"
    },
    "_inputs": {
      "rows": {
        "type": "array",
        "options": {
          "structures": {
            "values": [
              {
                "_inputs": {
                  "label": {
                    "comment": "Main text for this entry."
                  },
                  "type_markdown": {
                    "label": "Type",
                    "comment": "Supporting text next to the **Label** used to indicate the data type for this entry."
                  },
                  "inner_markdown": {
                    "label": "Content",
                    "comment": "Body text describing this entry."
                  }
                },
                "value": {
                  "label": "",
                  "type_markdown": "",
                  "inner_markdown": ""
                }
              }
            ]
          }
        }
      }
    },
    "params": {
      "rows": {
        "parser": "repeating",
        "options": {
          "editor_key": "rows",
          "snippet": "  <comp.DataReferenceRow [[row_args]]>\n[[content]]\n  </comp.DataReferenceRow>\n"
        }
      },
      "row_args": {
        "parser": "key_values",
        "options": {
          "models": [
            {
              "editor_key": "label",
              "type": "string"
            },
            {
              "editor_key": "type_markdown",
              "type": "string"
            }
          ],
          "format": {
            "ref": "mdx_format"
          }
        }
      },
      "content": {
        "parser": "content",
        "options": {
          "indented_by": "    ",
          "editor_key": "inner_markdown",
          "allow_nested": false
        }
      }
    }
  }
}
