{
  "tabs": {
    "snippet": "<comp.Tabs [[args]]>[[tabs]]</comp.Tabs>",
    "inline": false,
    "preview": {
      "text": "Tabs",
      "subtext": {
        "key": "label"
      }
    },
    "_inputs": {
      "inner_markdown": {
        "type": "markdown",
        "options": {
          "blockquote": true,
          "bold": true,
          "italic": true,
          "strike": true,
          "subscript": true,
          "superscript": true,
          "underline": true,
          "link": true,
          "bulletedlist": true,
          "numberedlist": true,
          "code": true,
          "embed": true,
          "horizontalrule": true,
          "image": true,
          "table": true,
          "snippet": true
        }
      }
    },
    "params": {
      "args": {
        "parser": "key_values",
        "options": {
          "models": [
            {
              "editor_key": "label",
              "type": "string"
            }
          ],
          "format": {
            "ref": "mdx_format"
          }
        }
      },
      "tabs": {
        "parser": "repeating",
        "options": {
          "editor_key": "tabs",
          "snippet": "<comp.Tab [[tab_args]]>[[content]]</comp.Tab>",
          "style": {
            "output": "block",
            "between": "\n",
            "block": {
              "leading": "\n",
              "trailing": "\n",
              "indent": "  "
            }
          }
        }
      },
      "tab_args": {
        "parser": "key_values",
        "options": {
          "models": [
            {
              "editor_key": "name",
              "type": "string"
            }
          ],
          "format": {
            "ref": "mdx_format"
          }
        }
      },
      "content": {
        "parser": "content",
        "options": {
          "editor_key": "inner_markdown",
          "allow_nested": true,
          "style": {
            "output": "block",
            "block": {
              "leading": "\n",
              "trailing": "\n",
              "indent": "  "
            }
          }
        }
      }
    }
  }
}