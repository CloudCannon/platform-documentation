{
  "docs_video": {
    "template": "mdx_component",
    "inline": false,
    "preview": {
      "text": "Docs Video",
      "subtext": [
        {
          "key": "title"
        }
      ],
      "icon": "crop_original"
    },
    "_inputs": {
      "title": {
        "comment": "The title of the video, used for accessibility"
      },
      "url": {
        "comment": "The location of the video (Must be MP4)",
        "type": "url"
      },
      "type": {
        "type": "select",
        "options": {
          "values": ["screenshot", "ui-snippet"]
      },
      "autoplay": {
        "comment": "Whether this video should start playing automatically",
        "type": "switch"
      },
      "loop": {
        "comment": "Whether this video should start again when finished",
        "type": "switch"
      }
    },
    "definitions": {
      "component_name": "comp.DocsVideo",
      "named_args": [
        {
          "editor_key": "url",
          "type": "string"
        },
        {
          "editor_key": "title",
          "type": "string"
        },
        {
          "editor_key": "type",
          "type": "string",
          "optional": true,
          "remove_empty": true
        },
        {
          "editor_key": "autoplay",
          "optional": true,
          "default": true,
          "type": "boolean"
        },
        {
          "editor_key": "loop",
          "optional": true,
          "default": true,
          "type": "boolean"
        }
      ]
    }
  }
}