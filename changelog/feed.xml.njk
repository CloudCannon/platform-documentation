---
url: /changelog/feed.xml
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<link href="{{ url | url(true) }}" rel="self"/>
	<link href="{{ "/changelog" | url(true) }}"/>
	{% set pages = search.pages("changelogs", "date=desc") %}
	{% set lastPage = pages | first %}
	<updated>{{ lastPage.date | date("ATOM") }}</updated>
	<id>{{ url | url(true) }}</id>
	<title>{{ meta.title }}</title>
	<subtitle>{{ meta.subtitle }}</subtitle>
	{%- for entry in pages %}
	<entry>
		<title>{{ entry.title }}</title>
		<link href="{{ entry.url | url(true) }}"/>
		<updated>{{ entry.date | date("ATOM") }}</updated>
		<id>{{ entry.url | url(true) }}</id>
		{% set content = entry.content %}
		<content type="html"><![CDATA[ {{ content | md | safe }} ]]></content>
		<summary type="html"><![CDATA[ {{ content | md | changelog_summary | safe}} ]]></summary>
	</entry>
	{%- endfor %}
</feed>