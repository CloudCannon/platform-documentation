{% macro objectNav(propDoc,doc) %}
  {% from "nav/any-of.njk" import anyOfNav with context %}
  <ol class="t-docs-nav__sub-list">
    {% for innerKey, innerPropRef in propDoc.properties %}
      {% set innerPropDoc = innerPropRef | get_docs_by_ref %}
      {% if innerPropDoc.url == doc.url %}
        <li>
          <span class="t-docs-nav__sub-list__article" aria-current="page">{{ innerKey }}</span>
        </li>
      {% elif innerPropDoc.url %}
        <li>
          <a class="t-docs-nav__sub-list__article" href="/documentation/developer-reference/cloudcannon-configuration-file{{ innerPropDoc.url }}">{{ innerKey }}</a>
        </li>
      {% endif %}
    {% endfor %}

    {% for additionalPropRef in propDoc.additionalProperties %}
      {% set additionalPropDoc = additionalPropRef | get_docs_by_ref %}
      {% if additionalPropDoc.url == doc.url %}
        <li>
          <span class="t-docs-nav__sub-list__article" aria-current="page">{{ additionalPropDoc.title | default(additionalPropDoc.key, true) }}</span>
        </li>
      {% elif additionalPropDoc.url %}
        <li>
          <a class="t-docs-nav__sub-list__article" href="/documentation/developer-reference/cloudcannon-configuration-file{{ additionalPropDoc.url }}">{{ additionalPropDoc.title | default(additionalPropDoc.key, true) }}</a>
        </li>
      {% endif %}
    {% endfor %}

    {% if propDoc.additionalProperties | length == 1 %}
      {% set propDoc = propDoc.additionalProperties[0] | get_docs_by_ref %}
      {% if propDoc.anyOf | length %}
        <li>{{ anyOfNav(propDoc,doc) }}</li>
      {% endif %}
    {% endif %}
  </ol>
{% endmacro %}