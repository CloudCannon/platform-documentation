---
layout: layouts/base.njk
---

<div class="l-page">
    <div class="l-column">
        <div class="u-card-box l-small-content">
            <p class="changelog-entry__eyebrow">Changelog</p>
            <h1 data-pagefind-body class="l-heading changelog-entry__heading">{{ title }}</h1>
            <p class="changelog-entry__date">{{ date | DATE_TO_NOW }}</p>
            <main data-pagefind-body data-pagefind-filter="site:Changelog" data-pagefind-meta="site:Changelog">
                {{ content | safe }}
            </main>
        </div>
    </div>
</div>