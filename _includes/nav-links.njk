<ol class="l-header__links">
  {% for item in headingnav.items %}
    <li><a 
      {% if item.href in url %} aria-current="page"{% endif %}
      href="{{ item.href }}">{{ item.text }} {% if item.items | length %}<img src="{{ "chevron_right:filled" | icon("material") }}" inline>{% endif %}</a></li>
  {% endfor %}
</ol>
